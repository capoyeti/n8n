    {
      "id": "filter-pending",
      "name": "Filter Pending Only",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        440,
        0
      ],
      "parameters": {
        "jsCode": "// Filter leads with campaign_status = 'pending'\n// These are leads that haven't been contacted yet\n\nconst leads = $input.all();\nconst pendingLeads = [];\n\nfor (const item of leads) {\n  const lead = item.json;\n  \n  // Only include leads with pending status (or empty status)\n  const status = lead.campaign_status?.toLowerCase() || 'pending';\n  if (status === 'pending' || status === '') {\n    // Must have a valid phone number\n    if (lead.phone_normalized && lead.phone_normalized.length >= 10) {\n      pendingLeads.push({\n        json: {\n          ...lead,\n          sequence_step: 1 // Starting at step 1\n        }\n      });\n    }\n  }\n}\n\n// Limit to 50 per campaign run to avoid WhatsApp rate limits\nreturn pendingLeads.slice(0, 50);"
      }
    },
