    {
      "parameters": {
        "jsCode": "const lead = $input.first().json;\nconst templates = $('Get Templates').all();\n\nconst template = templates.find(t => parseInt(t.json.step) === 1);\n\nif (!template) {\n  return [{ json: { error: 'No template found for step 1', lead } }];\n}\n\nlet message = template.json.message || '';\nmessage = message.replace(/\\{\\{first_name\\}\\}/g, lead.first_name || 'there');\nmessage = message.replace(/\\{\\{business_name\\}\\}/g, lead.business_name || '');\nmessage = message.replace(/\\{\\{city\\}\\}/g, lead.city || '');\nmessage = message.replace(/\\{\\{industry\\}\\}/g, lead['what_industry_do_you_work_in?'] || '');\n\nreturn [{\n  json: {\n    lead_id: lead.id,\n    phone: lead.phone_normalized,\n    email: lead.email,\n    first_name: lead.first_name,\n    business_name: lead.business_name,\n    message: message,\n    sequence_step: 1,\n    template_name: template.json.name\n  }\n}];"
      },
