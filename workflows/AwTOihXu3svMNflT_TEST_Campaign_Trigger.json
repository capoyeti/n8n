    {
      "parameters": {
        "jsCode": "// Filter leads with campaign_status = 'pending'\nconst leads = $input.all();\nconst pendingLeads = [];\n\nfor (const item of leads) {\n  const lead = item.json;\n  const status = lead.campaign_status?.toLowerCase() || 'pending';\n  if (status === 'pending' || status === '') {\n    if (lead.phone_normalized && lead.phone_normalized.length >= 10) {\n      pendingLeads.push({\n        json: {\n          ...lead,\n          sequence_step: 1\n        }\n      });\n    }\n  }\n}\n\n// Limit to 50 per campaign run\nreturn pendingLeads.slice(0, 50);"
      },
